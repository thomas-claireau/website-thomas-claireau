{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "header": {
      "type": "component",
      "repeatable": false,
      "component": "global.header"
    },
    "technologies": {
      "collection": "technologie"
    },
    "content": {
      "type": "richtext"
    },
    "slug": {
      "type": "string"
    },
    "time": {
      "type": "integer",
      "default": 5
    }
  }
}
